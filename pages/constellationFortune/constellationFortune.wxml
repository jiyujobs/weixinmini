<!--pages/constellationFortune/constellationFortune.wxml-->
<view class="container" wx:for="{{obj}}">
  <view class='title'>星座运势</view>
  <view class='title'>{{item.astroname}}</view>
  <view class="image" >
    <image src="../../images/{{item.astroname}}.png"></image>
  </view>
  <view class="bar">
  <view class="bar1">
    <view class="box">幸运颜色:</view> 
    <view class="box1">{{item.color}}</view>
  </view>
  <view class="bar1">
   <view class="box">贵人星座:</view>
    <view class="box1">{{item.star}}</view>
  </view>
  <view class="bar1"> 
    <view class="box">幸运数字:</view>
      <view class="box1">{{item.number}}</view>
  </view>
</view>
  <view class="image">
    <image src="../../images/{{item.star}}.png"></image>
  </view>
</view>
