/* pages/fortune/fortune.wxss */
.container{
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.detail{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.bar{
  width: 100%;
  display: flex;
  flex-direction: row;
  margin: 20rpx 0;
}
.box{
  width: 50%;
  text-align: center;
}
.image{
  justify-content: center;
  display: flex;
}
.image image{
  width: 3em;
  height: 3em;
}